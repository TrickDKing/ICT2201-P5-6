{
  "name": "smartpantry",
  "version": "1.0.0",
  "description": "A web app created to aid people at APSN Cafe for Tech 4 Good challenge 2021.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "frontend": "cd frontend && npm start",
    "backend": "cd backend && venv\\Scripts\\activate && python app.py",
    "installFrontend": "cd frontend && npm install",
    "installBackend": "cd backend && py -3.9 -m venv venv && venv\\Scripts\\activate && pip install -r requirements.txt",
    "dev": "concurrently -n 'backend,frontend' -c 'blue,green' \"npm run backend\" \"npm run frontend\"",
    "install": "concurrently \"npm run installFrontend\" \"npm run installBackend\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CarltonFoo/ICT2103-SmartPantry.git"
  },
